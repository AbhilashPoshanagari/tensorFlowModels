{
  "compileOnSave": false,
  "compilerOptions": {
	"baseUrl": "./",
	//"baseUrl": "https://abhilashposhanagari.github.io/tensorFlowModels",
   // "outDir": "./dist/out-tsc",
   "outDir": "./docs",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
	"skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
